:activityType :activityTypeName """QIAamp 96 DNA QIAcube HT""" .

{ ?p :a :Plate .
  ?p :is :NotStarted .
} => {
  :step :stepTypeName """Create stamped plate""" .
  :step :addFacts {
    ?p :is :Started .
    ?p :transferTo ?q .
  } .
  :step :removeFacts {
    ?p :is :NotStarted .
  } .
  :step :createAsset {
    ?q :a :Plate .
    ?q :a :StampedPlate .
  } .
} .

{ ?p :a :StampedPlate .
} => {
  :step :stepTypeName """Create Biorobot plate""" .
  :step :addFacts {
    ?p :transferTo ?q .
  } .
  :step :createAsset {
    ?q :a :Plate .
    ?q :a :BiorobotPlate .
  } .
} .

{ ?p :a :BiorobotPlate .
} => {
  :step :stepTypeName """Create Stock plate""" .
  :step :addFacts {
    ?p :transferTo ?q .
  } .
  :step :createAsset {
    ?q :a :Plate .
    ?q :a :StockPlate .
  } .
} .

{ ?p :a :StockPlate .
} => {
  :step :stepTypeName """Perform QC""" .
  :step :addFacts { ?p :has :QCPerformed .} .
} .
