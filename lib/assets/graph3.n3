:activity :activityTypeName """Testing activity type""" .

{ ?p :is :Tube .} => { :step :stepTypeName """Get inputs""" .
       :step :addFacts { ?p :is :Tube1D .} .
       :step :removeFacts { ?p :is :Tube .} .
      }.

{ ?p :is :Tube1D .} => { :step :stepTypeName """Obtain outputs""" .
        :step :createAsset { ?q :is :EmptyTube2D .} .
        :step :selectAsset { ?q :is :EmptyTube2D .} .
      }.

{ ?p :is :Tube1D .
  ?q :is :EmptyTube2D .
} => { :step :stepTypeName """Transfer 1D to 2D tubes""" .
        :step :removeFacts {
          ?p :is :Tube1D .
          ?q :is :EmptyTube2D .
        } .
        :step :addFacts {
          ?p :is :EmptyTube1D .
          ?q :is :Tube2D .
          ?p :transferAliquots ?q .
        } .
      }.
