<div class="alert panel panel-default" data-psd-component-class="AssetFacts" data-psd-component-parameters='<%= facts %>'>
  <div class="panel-heading">
    <h3 class="panel-title">
    <span class="" >
      <span data-psd-button-switch-content="open" data-psd-button-switch-selected="<%= keepSelected %>" class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
    </span>

      <span data-psd-condition-group-title="true" data-psg-editable-text-regexp="^[a-zA-Z][a-zA-Z0-9]*$"
      data-psd-component-class="EditableText"><%= name %></span><button data-condition-group-remove="1" data-dismiss="alert"  class="close pull-right ">&times;</button>
    </h3>

  </div>
  <div class="panel-body">
    <small>Actions</small>
    <div class="facts">
      This asset has no annotations yet.
    </div>
  </ul>
  </div>

  <div class="panel-footer">

    <div class="input-group" data-psd-component-class="FactReader" data-psd-component-parameters='{ "actionType" : "addFacts" }'>
      <div class="input-group-btn" data-psd-fact-reader-operation-selector="true">
        <button disabled="disabled" data-psd-fact-reader-action-type="addFacts" class="btn btn-default">
          <span data-psd-fact-reader-button-text="true" class="glyphicon glyphicon-pencil" aria-hidden="true"></span> addFacts
        </button>
      </div>
      <input data-psd-fact-reader-text-input="true" type="text" name="object"
      class="form-control" aria-label="...">
      <div class="input-group-btn">
        <button data-psd-fact-reader-add="true" type="button" class="btn btn-default form-control">Add</button>
      </div>
    </div>

  </div>

</div>
